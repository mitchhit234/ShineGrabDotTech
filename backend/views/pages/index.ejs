  <body>
<div class="px-4 py-5 my-5 text-center">
  <img class="d-block mx-auto mb-4" src="../assets/brand/bootstrap-logo.svg" alt="" width="72" height="57">
  <h1 class="display-5 fw-bold">Basic Gameplay Stats</h1>
  <div class="col-lg-6 mx-auto">
    <p class="lead mb-4">Here's a brief overview of your gameplay.</p>
    <div class="d-grid gap-2 justify-content-sm-center">
      <!-- INJECT HTML HERE FROM JS -->
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Neutral Wins</th>
            <th scope="col">Actions Per Minute</th>
            <th scope="col">Damage Per Kill</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td scope="row"></th>
            <td ></td>
            <td ></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>

<div class="b-example-divider"></div>

<div class="px-4 pt-5 my-5 text-center">
  <h1 class="display-5 fw-bold">Wavedashes</h1>
  <div class="col-lg-6 mx-auto">
    <p class="lead mb-4">A summary of your wavedash timings.</p>
    <h2 class="whitespace">Acceptable Wavedash Percentage: </h2>
    <div class="d-grid gap-2  justify-content-sm-center mb-5">
       <!-- INJECT HTML HERE FROM JS -->
       <table class="table">
        <thead>
          <tr>
            <th scope="col">Starting Frame</th>
            <th scope="col">Reason for Missing</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% wavedashes.forEach(function(wavedash) { %>
              <td scope="row"><% wavedash.frameStart %></th>
              <td >wavedash.isGoodDash</td>
          <% }); %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="overflow-hidden" style="max-height: 30vh;">
    <div class="container px-5">
    </div>
  </div>
</div>

<div class="b-example-divider"></div>

<div class="px-4 pt-5 my-5 text-center">
  <h1 class="display-5 fw-bold">Techs</h1>
  <div class="col-lg-6 mx-auto">
    <p class="lead mb-4">A summary of your tech skill.</p>
    <h2 class="whitespace">Acceptable Tech Percentage: </h2>
    <div class="d-grid gap-2  justify-content-sm-center mb-5">
       <!-- INJECT HTML HERE FROM JS -->
       <table class="table">
        <thead>
          <tr>
            <th scope="col">Starting Frame</th>
            <th scope="col">Reason for Missing</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% techs.forEach(function(tech) { %>
              <td scope="row"><% tech.frameStart %></th>
              <td ><% tech.techCase %></td>
            <% };) %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="overflow-hidden" style="max-height: 30vh;">
    <div class="container px-5">
    </div>
  </div>
</div>

<div class="b-example-divider"></div>

<div class="px-4 pt-5 my-5 text-center">
  <h1 class="display-5 fw-bold">Shine Grabs</h1>
  <div class="col-lg-6 mx-auto">
    <p class="lead mb-4">An analysis of your shine grab skill.</p>
    <div class="d-grid  justify-content-sm-center mb-5">
       <!-- INJECT HTML HERE FROM JS -->
       <table class="table">
        <thead>
          <tr>
            <th scope="col">Starting Frame</th>
            <th scope="col">Acceptable</th>
            <th scope="col">Reason for Missing</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% shinegrabs.forEach(function(shinegrab) { %>
              <td scope="row"><% shinegrab.frameStart %></th>
              <td ><% tech.successful %></td>
              <td ><% tech.isPerfectJump %></td>
              <td ><% tech.isPerfectGrab %></td>
            <% }); %>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="overflow-hidden" style="max-height: 30vh;">
    <div class="container px-5">
    </div>
  </div>
</div>


<div class="b-example-divider mb-0"></div>
    <script src="bootstrap.bundle.min.js"></script>
  </body>
</html>
