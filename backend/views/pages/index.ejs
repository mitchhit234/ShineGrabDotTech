    <link rel="canonical" href="https://getbootstrap.com/docs/5.0/examples/heroes/">

    
<link href="views/bootstrap.min.css" rel="stylesheet">

    <style>
      .bd-placeholder-img {
        font-size: 1.125rem;
        text-anchor: middle;
        -webkit-user-select: none;
        -moz-user-select: none;
        user-select: none;
      }

      .whitespace {
        margin-bottom: 2.5em;
      }

      @media (min-width: 768px) {
        .bd-placeholder-img-lg {
          font-size: 3.5rem;
        }
      }

      .table tr {
        text-align: center;
        font-size: 20px;
        padding: 0px;
      } 

      .float-left {
        transform: rotate(45deg);
      }

      .images {
        max-width: 90%;
        border-style: solid;
        border-color: #5a5a5a;
        border-radius: 2em;
      }

      #container {height: 100%; width:100%; font-size: 0;}
      #left, #middle, #right {display: inline-block; *display: inline; zoom: 1; vertical-align: center; font-size: 30px;}
      #left {width: 25%;}
      #middle {width: 50%; font-size: 50px; padding-left: 10px; padding-right: 10px;}
      #right {width: 25%;}

    </style>

    
    
    <link href="views/heroes.css" rel="stylesheet">
  </head>
  <body>

<div class="px-4 py-5 my-5 text-center">
  <div id="container">
    <div id="left" class="display-2 fw-bold">Stage - Dreamland</div>
    <h1 id="middle" class="display-5 fw-bold">Basic Gameplay Stats</h1>
    <div class="display-2 fw-bold" id="right">Tip - Average % Damage Per KO</div>
  </div>
    <p class="lead mb-4">Here's a brief overview of your gameplay.</p>
    
    <div id="container">
      <div id=left><img class="images" src="views/images/dreamland.png"/></div>
    <div id= "middle"> 
      <div class="d-grid gap-2  justify-content-sm-center mb-5">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">Percent of Neutral Wins</th>
            <th scope="col">Inputs Per Minute</th>
            <th scope="col">Average % Damage Per KO</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= neutral %>%</td>
            <td><%= inputspm %></td>
            <td><%= damagepk %>%</td>
          </tr>
        </tbody>
      </table>
      </div>
    </div>
    <div id="right" style="font-size: 23px; vertical-align:middle">Falco has a poor recovery. Try and edge guard him and your damage per KO percentage will decrease </div>
  </div>
</div>

<div class="b-example-divider"></div>

<div class="px-4 pt-5 my-5 text-center">
  <div id="container">
    <div id="left" class="display-2 fw-bold">Tip - Something</div>
    <h1 id="middle" class="display-5 fw-bold">Wavedashes</h1>
    <div class="display-2 fw-bold" id="right"></div>
    </div>
    <p class="lead mb-4 text-center">A summary of your wavedash timings.</p>
    <h2 class="whitespace">Acceptable Wavedash Percentage: <%= wdpercentage %>%</h2>
      <div id="container">
        <div id=left>Tip goes here</div>
        <div id= "middle">
       <div class="d-grid gap-2  justify-content-sm-center mb-5">

       <table class="table">
        <thead>
          <tr>
            <th scope="col">Starting Frame</th>
            <th scope="col">Reason for Missing</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% wavedashes.forEach(function(wavedash) { %>
              <% if (wavedash.isGoodDash == false) { %>
                <td scope="row"><%= wavedash.frameStart %>%</td>
                <td>Late</td>
              <% } %>
              </tr>
            <% }); %>         
        </tbody>
      </table>
    </div>
  </div>
  <div id="right" style="font-size: 23px; vertical-align:middle">Picture goes here </div>
    </div>
</div>

<div class="b-example-divider"></div>

<div class="px-4 py-5 my-5 text-center">
  <div id="container">
    <div id="left" class="display-2 fw-bold">Stage - Dreamland</div>
    <h1 id="middle" class="display-5 fw-bold">Techs</h1>
    <div class="display-2 fw-bold" id="right">Tip - Average % Damage Per KO</div>
  </div>
    <p class="lead mb-4">A summary of your tech skill</p>
    <h2 class="whitespace">Your Tech Percentage: <%= techcalc %>% </h2>
    <div id="container">
      <div id=left><img class="images" src="views/images/dreamland.png"/></div>
    <div id= "middle"> 
      <div class="d-grid gap-2  justify-content-sm-center mb-5">
       
       <table class="table">
        <thead>
          <tr>
            <th scope="col">Starting Frame</th>
            <th scope="col">Reason for Missing</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% techs.forEach(function(tech) { %>
              <td scope="row"><%= tech.frameStart %></td>
              <% if (tech.techCase == 0) { %>
                <td>No Press</td>
              <% } else if (tech.techCase == 1) { %>
                <td>Late Press</td>
              <% } else if (tech.techCase == -1) { %>
                <td>Early Press</td>
              <% } %>
              </tr>
            <% }); %>         
        </tbody>
      </table>
    </div>
  </div>
  <div id="right" style="font-size: 23px; vertical-align:middle">Picture goes here </div>
    </div>
 </div>
 

<div class="b-example-divider"></div>

<div class="px-4 pt-5 my-5 text-center">
  <div id="container">
    <div id="left" class="display-2 fw-bold">Tip - Something</div>
    <h1 id="middle" class="display-5 fw-bold">Shine Grabs</h1>
    <div class="display-2 fw-bold" id="right"></div>
    </div>
    <p class="lead mb-4 text-center">An analysis of your shine gra skill</p>
      <div id="container">
        <div id=left>Tip goes here</div>
        <div id= "middle">
       <div class="d-grid gap-2  justify-content-sm-center mb-5">
       
       <table class="table">
        <thead>
          <tr>
            <th scope="col">Starting Frame</th>
            <th scope="col">Type</th>
            <th scope="col">Error</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <% shinegrabs.forEach(function(shinegrab) { %>
              <td scope="row"><%= shinegrab.frameStart %></td>
              <% if (shinegrab.successful == true) { %>
              <% if (shinegrab.isPerfectJump == true && shinegrab.isPerfectGrab == true) { %>
                <td> Perfect</td>
                <td> None</td>
              <%}else if (shinegrab.isPerfectJump == false && shinegrab.isPerfectGrab == true) { %>
                <td>Acceptable</td>
                <td>Late Jump</td>
              <%}else if(shinegrab.isPerfectGrab == false) { %>
                <td>Failure</td>
                <td>Late Grab</td>
              <% }} %>
            </tr>
            <% }); %>          
        </tbody>
      </table>
    </div>
  </div>
  <div id="right" style="font-size: 23px; vertical-align:middle">Picture goes here </div>
    </div>
  </div>


<div class="b-example-divider"></div>


<div class="px-4 pt-5 my-5 text-center">
  <div id="container">
    <div id="left">Tip 1</div>
  <h1 id="middle" class="display-5 fw-bold">Miscellaneous</h1>
  <div id="right">Tip 2</div>
</div> 
<p class="lead mb-4">A collection of other useful data</p>
  <div class="col-lg-6 mx-auto">
    <div class="d-grid gap-2  justify-content-sm-center mb-5"> 

       <table class="table">
        <thead>
          <tr>
            <th scope="col">Percent of Neutral Wins</th>
            <th scope="col">Total Wave Dashes</th>
            <th scope="col">Inputs Per Minute</th>
            <th scope="col">Average Damage Per Kill</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= neutral %>%</td>
            <td><%= wavedashnum %></td>
            <td><%= inputspm %></td>
            <td><%= damagepk %></td>
          </tr>
        </tbody>
    </div>
  </div>

  <div class="overflow-hidden" style="max-height: 30vh;">
    <div class="container px-5">
    </div>
  </div>
</div>



    <script src="views/bootstrap.bundle.min.js"></script>
  </body>
</html>


